{
    "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
    "homepage": "https://github.com/PowerShell/PowerShell",
    "description": "PowerShell for every system! https://Microsoft.com/PowerShell",
    "version": "7.0.0-rc.2",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/PowerShell/PowerShell/releases/download/v7.0.0-rc.2/PowerShell-7.0.0-rc.2-win-x64.zip",
            "hash": "58c76bf77e206494e1fd82f96b65fc4bf240092dcb247c78077b63976262af41"
        },
        "32bit": {
            "url": "https://github.com/PowerShell/PowerShell/releases/download/v7.0.0-rc.2/PowerShell-7.0.0-rc.2-win-x86.zip",
            "hash": "ca90f07dd63b369bcd8b7ac20402af58fe812e782021b95396996ee280b9dad9"
        }
    },
    "bin": [
        "pwsh.exe",
        [
            "pwsh.exe",
            "pwsh-bare",
            "-nop -nol -noni -ex bypass"
        ]
    ],
    "shortcuts": [
        [
            "pwsh.exe",
            "PowerShell Core"
        ]
    ],
    "checkver": {
        "url": "https://github.com/PowerShell/PowerShell/releases",
        "re": "/releases/tag/(?:v)?([\\d.\\w.-]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/PowerShell/PowerShell/releases/download/v$version/PowerShell-$version-win-x64.zip"
            },
            "32bit": {
                "url": "https://github.com/PowerShell/PowerShell/releases/download/v$version/PowerShell-$version-win-x86.zip"
            }
        },
        "hash": {
            "url": "https://github.com/PowerShell/PowerShell/releases/tag/v$version/",
            "find": "$basename\\s*<ul>\\s*<li>([A-Fa-f\\d]{64})"
        }
    }
}
